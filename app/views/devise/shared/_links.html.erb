<%- if controller_name != 'sessions' %>
  <%= link_to t('authentication.login'), new_user_session_path %><br>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to t('authentication.signup'), new_user_registration_path %><br>
<% end %>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to t('devise.links.didn_t_receive_unlock_instructions'), new_unlock_path(resource_name) %><br>
<% end %>
